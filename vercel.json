{
  "version": 2,
  "builds": [
    { "src": "project/mySite/static", "use": "@vercel/static" },
    { "src": "project/mySite/LandingPage", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/project/mySite/static/$1" },
    { "src": "/(.*)", "dest": "/project/mySite/LandingPage/$1" }
  ]
}
